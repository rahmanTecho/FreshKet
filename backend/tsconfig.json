{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "baseUrl": ".",
      "types": ["jest"], // Include this line to add Jest types
      "paths": {
        "*": ["node_modules/*"]
      }
    },
    "include": ["src/**/*", "tests/**/*"], // Make sure your test folder is included
    "exclude": ["node_modules", "dist"]
  }
  